@article{omuraAmygdalaGrayMatter2005,
  title = {Amygdala Gray Matter Concentration Is Associated with Extraversion and Neuroticism},
  author = {Omura, Kazufumi and Todd Constable, R. and Canli, Turhan},
  date = {2005-11-28},
  journaltitle = {NeuroReport},
  volume = {16},
  number = {17},
