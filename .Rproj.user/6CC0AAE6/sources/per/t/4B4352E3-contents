@article{deyoungPersonalityNeuroscienceEmerging2022,
  title = {Personality Neuroscience: {{An}} Emerging Field with Bright Prospects},
  shorttitle = {Personality Neuroscience},
  author = {DeYoung, Colin G. and Beaty, Roger E. and Genç, Erhan and Latzman, Robert D. and Passamonti, Luca and Servaas, Michelle N. and Shackman, Alexander J. and Smillie, Luke D. and Spreng, R. Nathan and Viding, Essi and Wacker, Jan},
  date = {2022-10-12},
  journaltitle = {Personality Science},
  shortjournal = {Personal. Sci.},
  volume = {3},
  pages = {e7269},
  issn = {2700-0710},
  doi = {10.5964/ps.7269},
  url = {https://ps.psychopen.eu/index.php/ps/article/view/7269},
  urldate = {2022-10-23},
  abstract = {Personality neuroscience is the study of persistent psychological individual differences, typically in the general population, using neuroscientific methods. It has the potential to shed light on the neurobiological mechanisms underlying individual differences and their manifestation in ongoing behavior and experience. The field was inaugurated many decades ago, yet has only really gained momentum in the last two, as suitable technologies have become widely available. Personality neuroscience employs a broad range of methods, including molecular genetics, pharmacological assays or manipulations, electroencephalography, and various neuroimaging modalities, such as magnetic resonance imaging and positron emission tomography. Although exciting progress is being made in this young field, much remains unknown. In this brief review, we discuss discoveries that have been made, methodological challenges and advances, and important questions that remain to be answered. We also discuss best practices for personality neuroscience research and promising future directions for the field.},
