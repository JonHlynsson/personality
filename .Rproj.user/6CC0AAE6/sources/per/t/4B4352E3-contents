@article{holmesIndividualDifferencesAmygdalamedial2012,
  title = {Individual Differences in Amygdala-Medial Prefrontal Anatomy Link Negative Affect, Impaired Social Functioning, and Polygenic Depression Risk},
  author = {Holmes, Avram J. and Lee, Phil H. and Hollinshead, Marisa O. and Bakst, Leah and Roffman, Joshua L. and Smoller, Jordan W. and Buckner, Randy L.},
  date = {2012-12-12},
  journaltitle = {The Journal of Neuroscience: The Official Journal of the Society for Neuroscience},
